<textFilePopulator>
	<columnDelimiterRegex>\t</columnDelimiterRegex>
	<textFileColumn>
		<identifier>true</identifier>
		<header>sequenceId</header>
		<fieldName>sequenceID</fieldName>
	</textFileColumn>
	<textFileColumn>
		<header>conclusionTaxon</header>
		<fieldName>rega_genotype</fieldName>
		<matchPattern>(?:Geno_)?(\d+)(?:[a-z]+)?</matchPattern>
		<outputTemplate>${g1}</outputTemplate>
	</textFileColumn>
</textFilePopulator>