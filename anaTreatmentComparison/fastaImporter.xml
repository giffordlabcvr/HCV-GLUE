<fastaImporter>
	<sourceName>treatmentComparison</sourceName>
	<idParser>
		<!-- For the ID this matches the area just before _L001 or -L001 -->
		<matchPattern>(.+).L001</matchPattern>
		<outputTemplate>${g1}</outputTemplate>
	</idParser>
		<fieldParser>
		<fieldName>GENOTYPE</fieldName>
		<matchPattern>(?:[^\.]+\.)(\d+)</matchPattern>
		<outputTemplate>${g1}</outputTemplate>
	</fieldParser>
		<fieldParser>
		<fieldName>SUBTYPE</fieldName>
		<matchPattern>(?:[^\.]+\.)\d+([a-z]+)</matchPattern>
		<outputTemplate>${g1}</outputTemplate>
	</fieldParser>
</fastaImporter>