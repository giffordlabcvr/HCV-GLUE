<webAnalysisTool>
	<fastaSequenceReporterModuleName>fastaCodingOnlySequenceReporter</fastaSequenceReporterModuleName>
	<featureAnalysisHint>
		<featureName>precursor_polyprotein</featureName>
		<includesSequenceContent>true</includesSequenceContent>
		<variationScanHint>
			<referenceSequence>H77_AF009606</referenceSequence>
			<multiReference>true</multiReference>
			<descendentFeatures>true</descendentFeatures>
		</variationScanHint>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>Core</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>E1</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>E2</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>p7</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>NS2</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>NS3</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>NS4A</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>NS4B</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>NS5A</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>NS5B</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<variationCategory>
		<name>common_aa_polymorphisms</name>
		<displayName>Common amino acid polymorphisms</displayName>
		<description>Amino acids at a given codon location which appear in more than 1% of public HCV sequences</description>
		<whereClause>is_common_aa_polymorphism = 'true'</whereClause>
	</variationCategory>
	<variationCategory>
		<name>resistance_associated_variants</name>
		<displayName>Resistance-associated variants</displayName>
		<description>Amino acids at a given codon location which have been associated with drug resistance</description>
		<whereClause>is_resistance_associated_variant = 'true'</whereClause>
		<selectedByDefault>true</selectedByDefault>
	</variationCategory>
	<variationCategory>
		<name>epitopes_mhc_class_i</name>
		<displayName>MHC Class I Epitopes</displayName>
		<description>Linear HCV epitopes in MHC Class I (HLA-A, B, C, E, F, G, K, L)</description>
		<whereClause>is_epitope = 'true' and 
			epitope_full_hla != 'HLA-Class II, allele undetermined' and 
				(epitope_full_hla = 'HLA-Class I, allele undetermined' or 
				 epitope_full_hla like 'HLA-A%' or 
				 epitope_full_hla like 'HLA-B%' or 
				 epitope_full_hla like 'HLA-C%' or 
				 epitope_full_hla like 'HLA-E%' or 
				 epitope_full_hla like 'HLA-F%' or 
				 epitope_full_hla like 'HLA-G%' or 
				 epitope_full_hla like 'HLA-K%' or 
				 epitope_full_hla like 'HLA-L%')</whereClause>
	</variationCategory>
	<variationCategory>
		<name>epitopes_mhc_class_ii</name>
		<displayName>MHC Class II Epitopes</displayName>
		<description>Linear HCV epitopes in MHC Class II (HLA-DM, DO, DP, DP, DQ, DR)</description>
		<whereClause>is_epitope = 'true' and 
			( epitope_full_hla = 'HLA-Class II, allele undetermined' or 
				epitope_full_hla like 'HLA-D%' )</whereClause>
		<propertyTemplate>
			<name>name</name>
			<template>${name}</template>
		</propertyTemplate>
		<propertyTemplate>
			<name>referenceSequence</name>
			<template>${featureLoc.referenceSequence.name}</template>
		</propertyTemplate>
		<propertyTemplate>
			<name>feature</name>
			<template>${featureLoc.feature.name}</template>
		</propertyTemplate>
		<propertyTemplate>
			<name>pattern</name>
			<template>${regex!"-"}</template>
		</propertyTemplate>
		<propertyTemplate>
			<name>hla</name>
			<template>${epitope_full_hla!"-"}</template>
		</propertyTemplate>
		<propertyTemplate>
			<name>iedb_id</name>
			<template>${epitope_iedb_id!"-"}</template>
		</propertyTemplate>
		<propertyTemplate>
			<name>pubmed_id</name>
			<template>${pubmed_id!"-"}</template>
		</propertyTemplate>
		<propertyTemplate>
			<name>stage</name>
			<template>${epitope_stage!"-"}</template>
		</propertyTemplate>
		<propertyTemplate>
			<name>measurement</name>
			<template>${epitope_measurement!"-"}</template>
		</propertyTemplate>
		<propertyTemplate>
			<name>effector_tissue_type</name>
			<template>${epitope_effector_tissue_type!"-"}</template>
		</propertyTemplate>
		<propertyTemplate>
			<name>effector_cell_type</name>
			<template>${epitope_effector_cell_type!"-"}</template>
		</propertyTemplate>
		<propertyTemplate>
			<name>effector_cell_culture_condition</name>
			<template>${epitope_effector_cell_culture_condition!"-"}</template>
		</propertyTemplate>
	</variationCategory>
</webAnalysisTool>
