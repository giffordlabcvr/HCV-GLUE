<webAnalysisTool>
	<fastaSequenceReporterModuleName>fastaCodingOnlySequenceReporter</fastaSequenceReporterModuleName>
	<maxLikelihoodGenotyperModuleName>maxLikelihoodGenotyper</maxLikelihoodGenotyperModuleName>
	<featureAnalysisHint>
		<featureName>precursor_polyprotein</featureName>
		<includesSequenceContent>true</includesSequenceContent>
		<variationScanHint>
			<referenceSequence>REF_MASTER_NC_004102</referenceSequence>
			<multiReference>true</multiReference>
			<descendentFeatures>true</descendentFeatures>
		</variationScanHint>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>Core</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>E1</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>E2</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>p7</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>NS2</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>NS3</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>NS4A</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>NS4B</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>NS5A</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<featureAnalysisHint>
		<featureName>NS5B</featureName>
		<deriveSequenceAnalysisFrom>precursor_polyprotein</deriveSequenceAnalysisFrom>
	</featureAnalysisHint>
	<variationCategory>
		<name>uncommon_aa_hcv</name>
		<displayName>HCV-atypical substitutions</displayName>
		<description>Amino acids which appear at a given codon in HCV sequences at a frequency of less than 1%</description>
		<whereClause>name like 'uncommon_aa_%' and featureLoc.referenceSequence.name = 'REF_MASTER_NC_004102'</whereClause>
		<objectRendererModule>hcvUncommonAaRenderer</objectRendererModule>
	</variationCategory>
	<variationCategory>
		<name>uncommon_aa_genotype</name>
		<displayName>Genotype-atypical substitutions</displayName>
		<description>Amino acids which appear at a given codon in sequences with the same genotype at a frequency of less than 1%</description>
		<whereClause>name like 'uncommon_aa_%' and featureLoc.referenceSequence.alignmentsWhereRefSequence.displayName like 'HCV Genotype%'</whereClause>
		<objectRendererModule>hcvUncommonAaRenderer</objectRendererModule>
	</variationCategory>
	<variationCategory>
		<name>uncommon_aa_subtype</name>
		<displayName>Subtype-atypical substitutions</displayName>
		<description>Amino acids which appear at a given codon in sequences with the same subtype at a frequency of less than 1%</description>
		<whereClause>name like 'uncommon_aa_%' and featureLoc.referenceSequence.alignmentsWhereRefSequence.displayName like 'HCV Subtype%'</whereClause>
		<selectedByDefault>false</selectedByDefault>
		<objectRendererModule>hcvUncommonAaRenderer</objectRendererModule>
	</variationCategory>
	<variationCategory>
		<name>official_resistance_associated_variants</name>
		<displayName>Drug resistance (current clinical guidance)</displayName>
		<description>Substitutions with clinically-relevant drug resistance associations according to official EASL, AASLD and WHO guidelines</description>
		<whereClause>is_resistance_associated_variant = 'true' and drug_resistance_official_guidance.id != null</whereClause>
		<selectedByDefault>true</selectedByDefault>
		<cladeMatchProperty>drug_resistance_official_guidance.alignment.name</cladeMatchProperty>
		<objectRendererModule>hcvResistanceAssociatedVariantRenderer</objectRendererModule>
	</variationCategory>
	<variationCategory>
		<name>resistance_associated_variants</name>
		<displayName>Drug resistance (in vitro evidence)</displayName>
		<description>Substitutions with in vitro evidence for drug resistance</description>
		<whereClause>is_resistance_associated_variant = 'true'</whereClause>
		<selectedByDefault>false</selectedByDefault>
		<cladeMatchProperty>drug_resistance_finding.drug.drug_regimen_drug.drug_regimen.drug_regimen_alignment.alignment.name</cladeMatchProperty>
		<cladeMatchProperty>drug_resistance_finding.replicon_clade.name</cladeMatchProperty>
		<objectRendererModule>hcvResistanceAssociatedVariantRenderer</objectRendererModule>
	</variationCategory>
	<variationCategory>
		<name>epitopes_all</name>
		<displayName>All Epitopes</displayName>
		<description>All epitopes</description>
		<whereClause>is_epitope = 'true'</whereClause>
		<objectRendererModule>hcvEpitopeRenderer</objectRendererModule>
		<reportAbsence>true</reportAbsence>
	</variationCategory>
	<variationCategory>
		<name>epitopes_mhc_class_i</name>
		<displayName>MHC Class I Epitopes</displayName>
		<description>Epitopes in MHC Class I (HLA-A, B, C, E, F, G, K, L)</description>
		<whereClause>is_epitope = 'true' and 
			epitope_full_hla != 'HLA-Class II, allele undetermined' and 
				(epitope_full_hla = 'HLA-Class I, allele undetermined' or 
				 epitope_full_hla like 'HLA-A%' or 
				 epitope_full_hla like 'HLA-B%' or 
				 epitope_full_hla like 'HLA-C%' or 
				 epitope_full_hla like 'HLA-E%' or 
				 epitope_full_hla like 'HLA-F%' or 
				 epitope_full_hla like 'HLA-G%' or 
				 epitope_full_hla like 'HLA-K%' or 
				 epitope_full_hla like 'HLA-L%')</whereClause>
		<objectRendererModule>hcvEpitopeRenderer</objectRendererModule>
		<reportAbsence>true</reportAbsence>
	</variationCategory>
