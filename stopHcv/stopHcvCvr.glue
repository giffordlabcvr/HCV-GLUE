# file to import STOP-HCV CVR sequences, add them to the relevant alignments, and BLAST align 

create source stopHcvCvr

create module --fileName  stopHcvCvr-fastaImporter stopHcv/fastaImporter.xml 
module stopHcvCvr-fastaImporter import -f stopHcv/stopHcvCvr.fasta

multi-set field SEQUENCE -w "source.name = 'stopHcvCvr' and genotype != null" genotyping_method "Supplied in FASTA header"

run file stopHcv/addStopHcvCvrToAlignments.glue

